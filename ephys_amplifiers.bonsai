              <Value>-70</Value>
            </Expression>
            <Expression xsi:type="Combinator">
              <PropertyMappings>
                <PropertyMapping name="Channels" selector="Source2" />
              </PropertyMappings>
              <Combinator xsi:type="q2:SelectChannels">
                <q2:Channels>
