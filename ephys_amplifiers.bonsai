                <q2:Count>20</q2:Count>
              </Combinator>
            </Expression>
            <Expression xsi:type="Combinator">
              <PropertyMappings>
                <PropertyMapping name="Cutoff1" selector="Source2" />
              </PropertyMappings>
              <Combinator xsi:type="q2:FrequencyFilter">
                <q2:SamplingFrequency>30000</q2:SamplingFrequency>
